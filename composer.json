{
  "name": "kozz/static-cache",
  "type": "library",
  "description": "Static Cache",
  "keywords": ["Cache", "Static"],
  "homepage": "http://github.com/urakozz/php-static-cache",
  "license": "MIT",

  "authors": [
    {
      "name": "Yury Kozyrev",
      "email": "urakozz@gmail.com",
      "homepage": "https://github.com/urakozz"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "doctrine/cache":"*",
    "phpoption/phpoption":"*",
    "symfony/dependency-injection":"*"
  },

  "suggest": {
    "php": ">=5.6.5"
  },

  "autoload": {
    "psr-4": {
      "Kozz\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kozz\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "satooshi/php-coveralls": "*"
  },

  "minimum-stability": "stable"
}
